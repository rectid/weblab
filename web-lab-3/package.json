{
  "name": "social-admin-panel",
  "version": "1.0.0",
  "description": "Web-приложение для администрирования социальной сети на Node.js и Express с EJS и сборками Gulp/Webpack.",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon --watch server --watch data --ext js,json --exec node server/index.js",
    "build:gulp": "gulp build",
    "build:gulp:watch": "gulp watch",
    "build:webpack": "webpack --config webpack.config.js --mode production",
    "build:webpack:dev": "webpack --config webpack.config.js --mode development",
    "generate:cert": "node scripts/create-cert.js"
  },
  "keywords": [
    "express",
    "social-network",
    "admin",
    "ejs",
    "gulp",
    "webpack"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.21.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "del": "^7.1.0",
    "ejs-loader": "^0.5.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^10.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-ejs": "^5.1.0",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.1.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.1",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.47",
    "postcss-loader": "^7.3.3",
    "sass": "^1.80.3",
    "sass-loader": "^16.0.1",
    "selfsigned": "^2.4.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
